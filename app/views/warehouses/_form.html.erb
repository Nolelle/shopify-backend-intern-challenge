<%= form_with model: warehouse do |form| %>
 <div>
    <%= form.label :location %><br>
    <%= form.text_field :location %>
    <% warehouse.errors.full_messages_for(:location).each do |message| %>
     <div><%= message %></div>
     <% end %>
 </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>

<%= link_to "Home", root_path %>
<%= link_to "Warehouses", warehouses_path %>